{
	"name": "bolierplate-reactnative",
	"version": "0.0.1",
	"private": true,
	"author": "Gabriel Bursi",
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"dev": "react-native run android --active-arch-only",
		"test": "jest --passWithNoTests --maxWorkers=50% --verbose",
		"test:watch": "jest --watchAll --maxWorkers=25% --verbose",
		"generate:c": "plop --plopfile ./generators/component.mjs",
		"generate:s": "plop --plopfile ./generators/screen.mjs",
		"sb": "react-native start",
		"get-sb": "sb-rn-get-stories",
		"presb": "npm run get-sb",
		"storybook-watcher": "sb-rn-watcher",
		"rdt": "react-devtools",
		"lint": "eslint --ext .tsx --ext .ts src/",
		"prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
		"prettier:format": "prettier --check ./src"
	},
