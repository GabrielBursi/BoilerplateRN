		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"eslint --ext .tsx --ext .ts src/ --fix"
		],
		"./src/**": [
			"prettier --write ."
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"engines": {
		"node": ">=18"
	}
